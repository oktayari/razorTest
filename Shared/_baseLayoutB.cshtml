<!DOCTYPE html>
<html>
<head>
<title>WebPages Razor Demo Arge</title>
<!--[if IE]><script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
<link href="@Href("/Styles/SiteB.css")" rel="stylesheet" type="text/css" />

</head>
<body>
    <div id="wrapper">

        <div id="headerwrap">
            <div id="header">
                @RenderPage("/Shared/_Header.cshtml")
                @RenderSection("headerX", required: false)
            </div>
        </div>

        <div id="navigationwrap">
            <div id="navigation">
               @RenderPage("/Shared/_Search.cshtml")
            </div>
        </div>

        <div id="contentliquid">
            <div id="contentwrap">
                <div id="content">
                      @RenderBody()
                </div>
          </div>
        </div>

        <div id="leftcolumnwrap">
            <div id="leftcolumn">
                @RenderPage("/Shared/_Navigator.cshtml")
            </div>
        </div>

        <div id="rightcolumnwrap">
            <div id="rightcolumn">
                  @RenderSection("list", required: false)
            </div>
        </div>

        <div id="footerwrap">
            <div id="footer">
                @RenderPage("/Shared/_Footer.cshtml")
                 @RenderSection("footerX", required: false)
            </div>
        </div>

    </div>
</body>
</html>

